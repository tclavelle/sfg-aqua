	file = {Full Text PDF:/Users/Tyler-SFG/Zotero/storage/MUUAF7ZJ/Krkošek et al. - 2011 - Effects of parasites from salmon farms on producti.pdf:application/pdf;Snapshot:/Users/Tyler-SFG/Zotero/storage/Z7IWVJQK/14700.html:text/html}
}

@article{marty_relationship_2010,
	title = {Relationship of farm salmon, sea lice, and wild salmon populations},
	volume = {107},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/107/52/22599},
	doi = {10.1073/pnas.1009573108},
	abstract = {Increased farm salmon production has heightened concerns about the association between disease on farm and wild fish. The controversy is particularly evident in the Broughton Archipelago of Western Canada, where a high prevalence of sea lice (ectoparasitic copepods) was first reported on juvenile wild pink salmon (Oncorhynchus gorbuscha) in 2001. Exposure to sea lice from farmed Atlantic salmon (Salmo salar) was thought to be the cause of the 97\% population decline before these fish returned to spawn in 2002, although no diagnostic investigation was done to rule out other causes of mortality. To address the concern that sea lice from fish farms would cause population extinction of wild salmon, we analyzed 10–20 y of fish farm data and 60 y of pink salmon data. We show that the number of pink salmon returning to spawn in the fall predicts the number of female sea lice on farm fish the next spring, which, in turn, accounts for 98\% of the annual variability in the prevalence of sea lice on outmigrating wild juvenile salmon. However, productivity of wild salmon is not negatively associated with either farm lice numbers or farm fish production, and all published field and laboratory data support the conclusion that something other than sea lice caused the population decline in 2002. We conclude that separating farm salmon from wild salmon—proposed through coordinated fallowing or closed containment—will not increase wild salmon productivity and that medical analysis can improve our understanding of complex issues related to aquaculture sustainability.},
	language = {en},
	number = {52},
	urldate = {2017-11-13},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Marty, Gary D. and Saksida, Sonja M. and Quinn, Terrance J.},
	month = dec,
	year = {2010},
	pmid = {21149706},
	pages = {22599--22604},
	file = {Full Text PDF:/Users/Tyler-SFG/Zotero/storage/ZENSBM33/Marty et al. - 2010 - Relationship of farm salmon, sea lice, and wild sa.pdf:application/pdf;Snapshot:/Users/Tyler-SFG/Zotero/storage/5H6JWEV4/22599.html:text/html}
}

