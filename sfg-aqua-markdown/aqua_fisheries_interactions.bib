	journal = {Journal of Sea Research},
	author = {Sweet, Michael J. and Bateman, Kelly S.},
	month = oct,
	year = {2015},
	keywords = {Sea cucumber, Mollusca, Crustacea, Bacteria, Virus, Phage},
	pages = {16--32},
	file = {ScienceDirect Full Text PDF:/Users/Tyler-SFG/Zotero/storage/FKSECCGF/Sweet and Bateman - 2015 - Diseases in marine invertebrates associated with m.pdf:application/pdf;ScienceDirect Snapshot:/Users/Tyler-SFG/Zotero/storage/W7WNXF5E/S1385110115300186.html:text/html}
