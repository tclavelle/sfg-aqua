	author = {Jackson, Dave and Drumm, Alan and McEvoy, Sarah and Jensen, Østen and Mendiola, Diego and Gabiña, Gorka and Borg, Joseph A. and Papageorgiou, Nafsika and Karakassis, Yannis and Black, Kenneth D.},
	month = jan,
	year = {2015},
	keywords = {Aquaculture, Cage farming, Escapes, Prevent escape},
	pages = {21--26},
	file = {ScienceDirect Full Text PDF:/Users/Tyler-SFG/Zotero/storage/CPMYNJYB/Jackson et al. - 2015 - A pan-European valuation of the extent, causes and.pdf:application/pdf;ScienceDirect Snapshot:/Users/Tyler-SFG/Zotero/storage/M48AFQCN/S0044848614005481.html:text/html}
