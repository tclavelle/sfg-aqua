	pages = {471--496},
	file = {Full Text PDF:/Users/Tyler-SFG/Zotero/storage/SAUB4MEU/Lafferty et al. - 2015 - Infectious Diseases Affect Marine Fisheries and Aq.pdf:application/pdf}
}

