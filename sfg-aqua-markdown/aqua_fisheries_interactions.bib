@article{dolorosa_translocation_2013,
	title = {Translocation of {Wild} {Trochus} niloticus: {Prospects} for {Enhancing} {Depleted} {Philippine} {Reefs}},
	volume = {21},
	issn = {1064-1262},
	shorttitle = {Translocation of {Wild} {Trochus} niloticus},
	url = {http://dx.doi.org/10.1080/10641262.2013.800773},
	doi = {10.1080/10641262.2013.800773},
	abstract = {Intentional release of wild-caught individuals has been widely used to establish new populations of the commercially valuable but threatened reef gastropod Trochus niloticus in oceanic islands. Is this also a viable strategy to enhance depleted populations of this species and other marine invertebrates? We monitored growth and survival of 765 translocated individuals and 486 in their original habitat for 5–9 months. Individuals translocated to a severely overexploited reef (mainland Palawan) grew 2–3 times faster than those at Tubbataha Reefs Natural Park, Phillipines. Despite variations in growth between the three sites, survival probabilities were consistently high, ranging between 0.77 and 0.92. So translocation is feasible, and sites at which a species has previously been found are likely to be suitable for their growth and survival. If site management can control over-fishing, this approach is likely to be a valuable tool for enhancing field populations of a large invertebrates like Trochus that have a short lived planktonic larva.},
	number = {3-4},
	urldate = {2015-12-19},
	journal = {Reviews in Fisheries Science},
	author = {Dolorosa, Roger G. and Grant, Alastair and Gill, Jennifer A.},
	month = oct,
	year = {2013},
	pages = {403--413},
	file = {Snapshot:/Users/Tyler-SFG/Zotero/storage/6JI6SAAG/10641262.2013.html:text/html}
}

@article{kirkpatrick_killing_2010,
	title = {Killing {Tigers} to {Save} {Them}: {Fallacies} of the {Farming} {Argument}},
	volume = {24},
	issn = {1523-1739},
	shorttitle = {Killing {Tigers} to {Save} {Them}},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1523-1739.2010.01468.x/abstract},
	doi = {10.1111/j.1523-1739.2010.01468.x},
	abstract = {Abstract: The lucrative, illegal trade in tigers (Panthera tigris) remains a major conservation problem. Tiger farming has been proposed as a potential solution, with farmed tigers substituting for wild tigers. At first glance, this argument's logic seems simple: farming will increase the supply of tigers, prices will fall, and poaching will no longer be profitable. We contend, however, that this supply-side argument relies on mistaken assumptions. First, tiger markets are imperfect, meaning they are dominated by a few producers who control price. Second, consumers prefer wild tigers to farmed tigers and therefore the two are not pure substitutes. In economic terms, products from wild tigers are luxury goods, commanding a price premium. Third, there is no evidence that farmed tigers can be produced or sold more cheaply than wild tigers. In sum, it is unlikely that farming will drive down the price of wild-caught tigers or decrease profitability for tiger poachers. Rather, tiger farming is more likely to increase aggregate demand for tiger products and stimulate higher levels of poaching.},
	language = {en},
	number = {3},
	urldate = {2017-12-14},
	journal = {Conservation Biology},
	author = {Kirkpatrick, R. Craig and Emerton, Lucy},
	month = jun,
	year = {2010},
	keywords = {cacería furtiva, comercio ilegal de tigre, conservación de la oferta, crianza de vida silvestre, illegal tiger trade, poaching, supply-side conservation, wildlife farming},
	pages = {655--659},
	file = {Full Text PDF:/Users/Tyler-SFG/Zotero/storage/ZAX7UZIR/Kirkpatrick and Emerton - 2010 - Killing Tigers to Save Them Fallacies of the Farm.pdf:application/pdf;Snapshot:/Users/Tyler-SFG/Zotero/storage/VHFF4W5V/abstract.html:text/html}
}

