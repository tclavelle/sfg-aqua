	keywords = {Marine ecosystems, Mangrove swamps, Conservation science, Endangered species, Extinction risk, Species extinction, Ecosystems, aquaculture},
	pages = {e10095},
	file = {Full Text PDF:/Users/Tyler-SFG/Zotero/storage/JXUMZ672/Polidoro et al. - 2010 - The Loss of Species Mangrove Extinction Risk and .pdf:application/pdf;Snapshot:/Users/Tyler-SFG/Zotero/storage/2JPCFU2C/article.html:text/html}
