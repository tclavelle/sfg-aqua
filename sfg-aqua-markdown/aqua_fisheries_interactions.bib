@article{primavera_overcoming_2006,
	series = {Selected {Papers} {From} the {East} {Asian} {Seas} {Congress} 2003, {Putrajaya}, {Malaysia}},
	title = {Overcoming the impacts of aquaculture on the coastal zone},
