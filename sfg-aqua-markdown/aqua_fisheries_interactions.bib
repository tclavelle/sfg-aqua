@article{sanchez-jerez_aquacultures_2016,
	title = {Aquaculture’s struggle for space : the need for coastal spatial planning and the potential benefits of {Allocated} {Zones} for {Aquaculture} ({AZAs}) to avoid conflict and promote sustainability},
