@article{asche_market_2001,
	title = {Market interactions for aquaculture products},
	volume = {5},
	issn = {1365-7305},
	url = {http://dx.doi.org/10.1080/13657300109380296},
	doi = {10.1080/13657300109380296},
	abstract = {For two decades significant expansion in the volume of intensively farmed fish has occurred accompanied by substantial price reductions. This more competitive aquaculture sector has generated concerns over the impact on the market for fish. Market interactions for three groups of farmed fish species ‐ salmon, catfish and sea bass/sea bream are reviewed. Evidence suggests there is little substitution between farmed and cultured product, apart from species available both farmed and wild. Nonetheless the expansion of aquaculture supplies asserts growth in the market and the capture of market share from some other products. Existing data deficiencies may preclude identification of these changes and more detailed data especially from further along the value chain are required. Furthermore, the various unique attributes differentiating farmed product from captured may also explain the lack of integration. It is concluded that a combination of more refined price and non‐price data will be critical to future research.},
