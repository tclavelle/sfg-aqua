	month = nov,
	year = {2013},
	keywords = {Ecology, Environmental management, Atmospheric Sciences, Economic concentration, Environmental Engineering/Biotechnology, Environment, general, Global seafood markets, Physical Geography, Prices, Wild fisheries, aquaculture},
	pages = {937--950},
	file = {Full Text PDF:/Users/Tyler-SFG/Zotero/storage/Q7CFX2CF/Villasante et al. - 2013 - Why are Prices in Wild Catch and Aquaculture Indus.pdf:application/pdf;Snapshot:/Users/Tyler-SFG/Zotero/storage/SFQW3CKU/s13280-013-0449-8.html:text/html}
}

