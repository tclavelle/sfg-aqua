@article{skaala_interactions_1990,
	title = {Interactions betwen natural and farmed fish populations: information from genetic markers.},
	volume = {36},
	shorttitle = {Interactions betwen natural and farmed fish populations},
	doi = {10.1111/j.1095-8649.1990.tb05624.x},
	abstract = {Studies on genetic changes in farmed fish populations are reviewed, and the potential interactions between wild fish and farm escapees and between wild and stocked fish populations are discussed. Examples of the application of genetic markers in studies concerning survival and reproduction of stocked fish, and of genetic and ecological interactions between stocks, are given for brook trout (Salvelinus fontinalis), brown trout (Salmo trutta), rainbow trout (Oncorhynchus mykiss), cod (Gadus morhua), Guadeloupe bass (Micropterus treculi), walleye (Stizostedion vitreum vitreum) and chum salmon (Oncorhynchus keta). These studies produced different results. Evidence for successful reproduction and genetic interactions between released and wild stocks has been found in a few cases. Stocked populations sometimes had a lower reproductive success than wild populations. In 1 case, a population transferred from the wild to aquaculture failed to acclimatise, and was apparently lost from the gene pool in 2 generations.},
	language = {English},
	number = {3},
	journal = {Journal of Fish Biology},
